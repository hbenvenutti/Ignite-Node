{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@config/*": ["config/*"],
      "@errors/*": ["shared/errors/*"],
      "@utils/*": ["utils/*"],
      "@middlewares/*": ["shared/infra/http/middlewares/*"],
      "@providers/*": ["shared/container/providers/*"],
      "@shared/*": ["shared/*"],
      "@shared:routes/*": ["shared/infra/http/routes/*"],
      "@shared:typeorm/*": ["shared/infra/typeorm/*"],
      "@accounts:cases/*": ["modules/accounts/use-cases/*"],
      "@accounts:container/*": ["modules/accounts/infra/container/*"],
      "@accounts:dtos/*": ["modules/accounts/dtos/*"],
      "@accounts:entities/*": ["modules/accounts/infra/typeorm/entities/*"],
      "@accounts:mocks/*": ["modules/accounts/repositories/in-memory/*"],
      "@accounts:repos/*": ["modules/accounts/infra/typeorm/repositories/*"],
      "@accounts:irepos/*": ["modules/accounts/repositories/*"],
      "@accounts:routes/*": ["modules/accounts/infra/http/*"],
      "@accounts:types/*": ["modules/accounts/@types/*"],
      "@cars:cases/*": ["modules/cars/use-cases/*"],
      "@cars:container/*": ["modules/cars/infra/container/*"],
      "@cars:dtos/*": ["modules/cars/dtos/*"],
      "@cars:entities/*": ["modules/cars/infra/typeorm/entities/*"],
      "@cars:mocks/*": ["modules/cars/repositories/in-memory/*"],
      "@cars:repos/*": ["modules/cars/infra/typeorm/repositories/*"],
      "@cars:irepos/*": ["modules/cars/repositories/*"],
      "@cars:routes/*": ["modules/cars/infra/http/*"],
      "@cars:types/*": ["modules/cars/@types/*"],
      "@rental:cases/*": ["modules/rental/use-cases/*"],
      "@rental:container/*": ["modules/rental/infra/container/*"],
      "@rental:dtos/*": ["modules/rental/dtos/*"],
      "@rental:entities/*": ["modules/rental/infra/typeorm/entities/*"],
      "@rental:mocks/*": ["modules/rental/repositories/in-memory/*"],
      "@rental:repos/*": ["modules/rental/infra/typeorm/repositories/*"],
      "@rental:irepos/*": ["modules/rental/repositories/*"],
      "@rental:routes/*": ["modules/rental/infra/http/*"],
      "@rental:types/*": ["modules/rental/@types/*"]
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false
  }
}
